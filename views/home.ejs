<%- include("partials/header"); %>
<h1>Our Content</h1>
<center><em><%= homeStartingContents %> </em>
  <br>
  <div class="container" style="padding: auto; text-align: center;">
    <script src="https://apis.google.com/js/platform.js"></script>
    <div class="g-ytsubscribe" data-channelid="UCEp5dV9ZPf1kvrOA7SAN1rw" data-layout="full" data-count="default"></div>
  </div>
  <div class="container">
    <div class="row">
      
      <div class="card text-center col-sm">
        <h2><strong>Web development using HTML</strong></h2>
        <iframe src="https://www.youtube.com/embed/videoseries?list=PLUxcLTS1fiM1pUq9tqQC886ppo8pVIWNC" frameborder="0"
        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
        allowfullscreen></iframe>
      </div>
      
      <div class="card text-center col-sm">
        <h2><strong>DSA in C++</strong></h2>
        <iframe src="https://www.youtube.com/embed/videoseries?list=PLUxcLTS1fiM0iwtBDP0B3GRkPaAEFjdZf" frameborder="0"
        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
        allowfullscreen></iframe>
      </div>
      <div class="card text-center col-sm">
        <h2><strong>STL in C++ playlist</strong> </h2>
        <iframe src="https://www.youtube.com/embed/videoseries?list=PLUxcLTS1fiM2yLV2LYgBji8T7gGU_RMkU" frameborder="0"
        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
        allowfullscreen></iframe>
      </div>
    </div>
  </div>
  <br>
</center>
  
<!-- <img src="https://i.ibb.co/61yyjVz/logo.png" alt="coding wizards logo" class="logo"> -->
<div class="row">
<% newPost.forEach(function (params) {%>
<div class="container">
  <div class="row">
    <div class="col-10 col-sm-10 col-md-10 col-lg-10" style="margin: auto;">
      <div class="card">
        <div class="card-body">
          <h4 class="card-title"><%=params.title%></h4>
          <small class="text-muted cat">
            <i class="far fa-clock text-info"></i> 30 minutes
            <i class="fas fa-users text-info"></i> 4 portions
          </small>
          <p class="card-text"> <%= params.content.substring(0, 100)+"..."  %></p>
          <a href="/posts/<%=params._id%>" class="btn btn-info">Read More</a>
        </div>
        <div class="card-footer text-muted d-flex justify-content-between bg-transparent border-top-0">
          <div class="views"><%= params.date %> 
          </div>
          <div class="stats">
           	<p>Written by  ~<em><%= params.author %> </em></p>
          </div>
           
        </div>
      </div>
    </div>
  </div>
</div>
<% }); %>
</div>

<%- include("partials/footer"); %>